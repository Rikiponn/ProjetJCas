program
    a, b : integer;
    c,d : boolean;
    e : array[1..10] of integer;
    f : Array[1..10] of integer;
    g : array[1..5] of array[1..10] of integer;
    h : array[1..10] of array[1..5] of boolean;
begin
    a := 5;
    b := 6;
    c := true;
    d := false;
    e := 1;
    f := 10;
    g[1][1] := -5;
    h[10][5] := true;
end.

