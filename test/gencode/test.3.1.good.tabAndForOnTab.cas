program
    a : integer;
    b : array[1..10] of integer;
begin

	FOR a := 1 TO 10 DO 
        b[a] := a * 10;
    END;
	FOR a := 1 TO 10 DO 
        write(b[a], " ");
    END;
    new_line;


	FOR a := 10 DOWNTO 1 DO
        b[a] := a * 10;
    END;
	FOR a := 10 DOWNTO 1 DO
        write(b[a], " ");
    END;
    new_line;





end.

